<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/translucent_15_black"/>
            <stroke android:color="@color/personal_dialog_line"
                android:width="1px"/>
            <!-- 圆角 -->
            <corners
                android:radius="@dimen/common_btn_corners"/>
        </shape>

    </item>
    <item>
        <shape>
            <solid android:color="@android:color/transparent"/>
            <stroke android:color="@color/personal_dialog_line"
                android:width="1px"/>
            <!-- 圆角 -->
            <corners
                android:radius="@dimen/common_btn_corners"/>
        </shape>
    </item>
</selector>