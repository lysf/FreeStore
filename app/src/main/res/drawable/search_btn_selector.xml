<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <!--颜色-->
            <solid android:color="@color/translucent_20_black"/>
            <!-- 圆角 -->
            <corners
                android:radius="@dimen/common_btn_corners"/>
        </shape>

    </item>
    <item>
        <shape>
            <solid android:color="@color/common_window_bg"/>
            <stroke android:width="1px" android:color="@color/translucent_20_black" />
            <!-- 圆角 -->
            <corners
                android:radius="@dimen/common_btn_corners"/>
        </shape>
    </item>
</selector>